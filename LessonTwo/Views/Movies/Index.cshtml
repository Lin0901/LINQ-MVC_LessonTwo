@{
}

@model List<Movies>
    
    <h1>@ViewData["Title"]</h1>
    <h2>Movies Found: @ViewData["numOfMovies"]</h2>

    @foreach (Movies m in @Model)
    {
        <h2>@m.Title</h2>
        <div>@m.Category</div>
        <div>@m.Budget</div>
        <div>@m.Date.Year</div>
    }